<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:rpc-client="http://www.appcnd.com/schema/rpc-client"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd
       http://www.appcnd.com/schema/rpc-client
       http://www.appcnd.com/schema/rpc-client.xsd">

    <rpc-client:client id="qqq" zkAddress="127.0.0.1:2181" zkConnectionTimeout="5000" zkSessionTimeout="5000">
        <rpc-client:service>
            <rpc-client:name>com.appcnd.rpc.governance.sample.api.UserService</rpc-client:name>
            <rpc-client:version>1</rpc-client:version>
        </rpc-client:service>
    </rpc-client:client>
</beans>